@model SaleWebMvc.Models.Seller

@{
    ViewData["Title"] = "Seller";

}

<h2>@ViewData["Title"]</h2>

<div class="row">
    <div class="col-md-3">
        <form asp-action="Create">
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="control-label">
            </div>
            <div class="form-group">
                <label asp-for="Email" class="control-label"></label>
                <input asp-for="Email" class="control-label">
            </div>
            <div class="form-group">
                <label asp-for="BirthDate" class="control-label"></label>
                <input asp-for="BirthDate" class="control-label">
            </div>
            <div class="form-group">
                <label asp-for="BaseSalary" class="control-label"></label>
                <input asp-for="BaseSalary" class="control-label">
            </div>
            <div class="form-group">
                <input type="submit" value="Create New" class="btn btn-default">
            </div>
        </form>

    </div>

</div>